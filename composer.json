{
	"name": "tenantcloud/command-logger",
	"description": "Laravel package for saving command log information",
	"license": "MIT",
	"require": {
		"php": ">=8.2",
		"illuminate/support": "^9.0|^10.0",
		"illuminate/contracts": "^10.0|^11.0"
	},
	"require-dev": {
		"orchestra/testbench": "^8.5|^9.0",
		"pestphp/pest": "^2.8",
		"php-cs-fixer/shim": "^3.54",
		"tenantcloud/php-cs-fixer-rule-sets": "~3.3.1",
		"phpstan/phpstan": "~1.10.21",
		"phpstan/phpstan-phpunit": "^1.3",
		"phpstan/phpstan-webmozart-assert": "^1.2",
		"phpstan/phpstan-mockery": "^1.1",
		"nunomaduro/larastan": "^2.6"
	},
	"autoload": {
		"psr-4": {
			"TenantCloud\\CommandLogger\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"scripts": {
		"test": "vendor/bin/pest",
		"coverage": "XDEBUG_MODE=coverage vendor/bin/pest --coverage-html coverage",
		"cs-fix": "vendor/bin/php-cs-fixer fix -v --show-progress=dots",
		"phpstan": "vendor/bin/phpstan analyse --memory-limit=2G",
		"testbench": "vendor/bin/testbench"
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"config": {
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"TenantCloud\\CommandLogger\\CommandLoggerServiceProvider"
			]
		}
	}
}
