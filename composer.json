{
	"name": "tenantcloud/command-logger",
	"description": "Laravel package for saving command log information",
	"license": "MIT",
	"require": {
		"php": ">=8.3",
		"illuminate/support": "^9.0|^10.0|^11.0|^12.0",
		"illuminate/contracts": "^12.0"
	},
	"require-dev": {
		"orchestra/testbench": "^10.0",
		"pestphp/pest": "^3.8",
		"php-cs-fixer/shim": "^3.75",
		"tenantcloud/php-cs-fixer-rule-sets": "^3.4.1",
		"phpstan/phpstan": "^2.1.17",
		"phpstan/phpstan-phpunit": "^2.0",
		"phpstan/phpstan-webmozart-assert": "^2.0",
		"phpstan/phpstan-mockery": "^2.0",
		"nunomaduro/larastan": "^3.0"
	},
	"autoload": {
		"psr-4": {
			"TenantCloud\\CommandLogger\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"scripts": {
		"test": "vendor/bin/pest",
		"coverage": "XDEBUG_MODE=coverage vendor/bin/pest --coverage-html coverage",
		"cs-fix": "PHP_CS_FIXER_IGNORE_ENV=1 vendor/bin/php-cs-fixer fix -v --show-progress=dots",
		"phpstan": "vendor/bin/phpstan analyse --memory-limit=2G",
		"testbench": "vendor/bin/testbench"
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"config": {
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"TenantCloud\\CommandLogger\\CommandLoggerServiceProvider"
			]
		}
	}
}
